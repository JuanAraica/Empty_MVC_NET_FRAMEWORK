<!DOCTYPE html>
<html>
<head>
    <link rel="shortcut icon" href="~/Content/Images/Logo.jpg" type="image/x-icon">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Welcome to Hiking Guanacaste</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div class="container-for-admin">
        <!--Main Navigation-->
        <header>

            <!-- Navbar -->
            <nav class="navbar fixed-top navbar-expand-lg navbar-light white scrolling-navbar">
                <div class="container-fluid">

                    <!-- Brand -->
                    <a class="navbar-brand waves-effect" href="#" >
                        <strong class="blue-text">Haking Guanacaste</strong>
                    </a>

                    <!-- Collapse -->
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <!-- Links -->
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">

                        <!-- Left -->
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item">
                                <a class="nav-link waves-effect" href="#">
                                    Toures
 
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link waves-effect" href="#" >
                                    Destinos
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link waves-effect" href="#"  >
                                    Actividades
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link waves-effect" href="#"  >
                                    Atracciones
                                </a>
                            </li>
                        </ul>

                        <!-- Right -->
                        <ul class="navbar-nav nav-flex-icons">
                            <li class="nav-item">
                                <a href="#" class="nav-link waves-effect" >
                                    <i class="fab fa-facebook-f"></i>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link waves-effect"  >
                                    <i class="fab fa-twitter"></i>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="@Url.Action("SinLogin","VistasHtml")" class="nav-link border border-light rounded waves-effect"
                                    >
                                    <i class="fab fa-angle-right mr-2"></i>Log Out
                                </a>
                            </li>
                        </ul>

                    </div>

                </div>
            </nav>
            <!-- Navbar -->
            <!-- Sidebar -->
            <div class="sidebar-fixed position-fixed">

                <a class="logo-wrapper waves-effect">
                    <img src="~/Content/Images/Logo.jpg" class="img-fluid" alt="">
                </a>
                <div class="list-group list-group-flush">
                    <a href="@Url.Action("IndexAdmin","Actividad")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-money mr-3"></i>Actividades
                    </a>
                    <a href="@Url.Action("IndexAdmin","Atraccion")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Atracciones
                    </a>
                    <a href="@Url.Action("Index","Clientes")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Clientes
                    </a>
                    <a href="@Url.Action("IndexAdmin","Colaborador")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Colaboradores
                    </a>
                    <a href="@Url.Action("IndexAdmin","Cupon")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Cupones
                    </a>
                    <a href="@Url.Action("IndexAdmin","Destino")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Destinos
                    </a>
                    <a href="@Url.Action("IndexAdmin","Referencia")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Referenias y Opiniones
                    </a>
                    <a href="@Url.Action("IndexAdmin","Servicio")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Servicios
                    </a>
                    <a href="@Url.Action("IndexAdmin","Tour")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Toures
                    </a>
                    <a href="@Url.Action("IndexAdmin","Transporte")" class="list-group-item list-group-item-action waves-effect">
                        <i class="fa fa-pie-chart mr-3"></i>Transporte
                    </a>


                </div>

            </div>
